    "@babel/preset-react": "^7.0.0",
    "babel-loader": "8.0.6",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.3",
    "prop-types": "^15.7.2",
